.video{
  width: 50%;
  height: 320rpx;
  display: flex;
  flex-direction: column;
  margin-bottom: 10rpx;
}

.video image{
  width: 95%;
  height: 100%;
  border-radius: 10rpx;
}

.video .title{
  font-size: 12px;
  color: #666;
}

.video .viewcount{
  font-size: 10px;
  margin-top:5rpx;
  color: #999;
}